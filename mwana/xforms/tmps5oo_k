			<bind nodeset="/data/fac" type="xsd:int" />
			<bind nodeset="/data/title" type="xsd:string" constraint=". = 'CBA' or . = 'TN' or . = 'DA'" jr:constraintMsg="jr:itext('title-constraintMsg')" />
			<bind nodeset="/data/zone" type="xsd:string" relevant="/data/title = 'cba'" />
