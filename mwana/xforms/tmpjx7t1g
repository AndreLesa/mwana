<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa">
	<h:head>
		<h:title>My sweet form</h:title>
		<model>
			<instance>
				<data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://openrosa.org/formdesigner/F57909F3-D2B4-4175-B979-10CC56F27F4E" uiVersion="1" version="1" name="My sweet form">
					<name />
					<LOL />
					<foo />
				</data>
			</instance>
			<bind nodeset="/data/name" type="xsd:string" />
			<bind nodeset="/data/LOL" type="xsd:string" />
			<bind nodeset="/data/foo" type="xsd:string" />
			<itext>
				<translation lang="en" default="">
					<text id="name-label">
						<value>What is your NAME</value>
					</text>
					<text id="LOL-label">
						<value>lol</value>
					</text>
					<text id="foo-label">
						<value>foo</value>
					</text>
				</translation>
			</itext>
		</model>
	</h:head>
	<h:body>
		<input ref="/data/name">
			<label ref="jr:itext('name-label')" />
		</input>
		<input ref="/data/LOL">
			<label ref="jr:itext('LOL-label')" />
		</input>
		<input ref="/data/foo">
			<label ref="jr:itext('foo-label')" />
		</input>
	</h:body>
</h:html>